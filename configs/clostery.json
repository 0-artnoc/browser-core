{
  "platform": "react-native",
  "format": "common",
  "baseURL": "chrome://cliqz/content/",
  "settings": {
    "antitrackingButton": false,
    "telemetryProvider": "platform",
    "channel": "MI05",
    "HPN_CHANNEL": "ghostery"
  },
  "default_prefs" : {
    "attrackBloomFilter": true,
    "attrackForceBlock": true,
    "cliqz-adb-disk-cache": false,
    "cliqz-adb-abtest": true,
    "antitrackingBlocklist": "ghostery",
    "clostery.categoryPolicies": "{\"essential\": \"ALLOW_SAFE\"}"
  },
  "modules": [
    "core",
    "antitracking",
    "antitracking-blocker",
    "adblocker"
  ],
  "react_components": {
    "ExtensionApp": "./ExtensionApp"
  }
}
