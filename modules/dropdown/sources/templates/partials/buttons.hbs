{{#if internalResults}}
    <div class='buttons padded'>
        {{#if internalResultsLogo}}
            <span class="dot" style="background-color: #{{logo.backgroundColor}}"></span>
        {{/if}}
        {{#each (take internalResults internalResultsLimit)}}
            <a
                id="{{elementId}}"
                class="result btn {{className}}"
                data-url="{{ url }}"
                href="{{displayUrl}}"
            >
                {{local (concatLocale ../localePrefix title)}}
            </a>
        {{/each}}
    </div>
{{/if}}
